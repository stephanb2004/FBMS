{
  "multipart": [
    {
      "apply": {
        "model": "fbms:block/thin_black_curtain_post"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "fbms:block/thin_black_curtain_side"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "fbms:block/thin_black_curtain_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "fbms:block/thin_black_curtain_side_alt"
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "fbms:block/thin_black_curtain_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": false
      },
      "apply": {
        "model": "fbms:block/thin_black_curtain_noside"
      }
    },
    {
      "when": {
        "east": false
      },
      "apply": {
        "model": "fbms:block/thin_black_curtain_noside_alt"
      }
    },
    {
      "when": {
        "south": false
      },
      "apply": {
        "model": "fbms:block/thin_black_curtain_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": false
      },
      "apply": {
        "model": "fbms:block/thin_black_curtain_noside",
        "y": 270
      }
    }
  ]
}